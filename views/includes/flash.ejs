<div class="success">
  <% if(success && success.length){ %>
  <div class="alert alert-success col-6 offset-3" role="alert">
    <%=success%>
  </div>
  <%}%>
</div>

<div class="err">
  <% if(error.length ){ %>
  <div class="alert alert-danger col-6 offset-3" role="alert"><%=error%></div>

  <%}%>
</div>
